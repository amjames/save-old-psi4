#### UHF Oxygen energy, triplet
molecule mol {
    0 3
    O
    O 1 1.2
}

# DF case
set {
    basis cc-pVDZ
    guess sad
    soscf true
    scf_type df
    reference uhf
}

df_uhf_energy = energy('HF')
df_triplet_energy = -149.6286212486618865
compare_values(df_triplet_energy, df_uhf_energy, 6, 'DF-UHF Energy')

# Non-DF case
set {
    scf_type out_of_core
}

uhf_energy = energy('HF')
triplet_energy = -149.6289923133230104
compare_values(triplet_energy, uhf_energy, 6, 'UHF Energy')



